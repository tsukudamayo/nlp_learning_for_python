{
    "Recipe_Infos": [
        {
            "recipe_id": 3112,
            "recipe_name": "おひな様のちらし寿司",
            "recipe_name_kana": "オヒナサマノチラシズシ",
            "default_number_of_people_id": 4
        }
    ],
    "Recipe_Directions_Details": [
        {
            "directions_detail_pattern_id": 99963,
            "directions_detail_id": 98133,
            "sequence_no": 1,
            "cooking_procedure_description": "米は洗ってザルにあげ、水気を切っておく。炊飯器にすし飯用の水加減でセットし、上に昆布をのせて炊きあげる。 炊いている間に★印の調味料を合わせて塩と砂糖を良く溶かし寿司酢を作る。",
            "holiday_flag": 0,
            "classification_id": 13,
            "classification_name": "米下処理",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99964,
            "directions_detail_id": 98134,
            "sequence_no": 2,
            "cooking_procedure_description": "耐熱容器に干し椎茸とひたひたの水を入れて、ラップをせずに、レンジ600Wで20～30秒加熱して戻す。",
            "holiday_flag": 0,
            "classification_id": 6,
            "classification_name": "レンジ",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99965,
            "directions_detail_id": 98135,
            "sequence_no": 3,
            "cooking_procedure_description": "干し椎茸が柔らかくなったら石づきを取り、干し椎茸と一緒にＡと干し椎茸の戻し汁１/４カップを鍋に入れ、弱火で水分がなくなるまで煮て冷ます。冷めたら２/３量を細切り、残りをみじん切りにする。",
            "holiday_flag": 0,
            "classification_id": 9,
            "classification_name": "煮る",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99966,
            "directions_detail_id": 98136,
            "sequence_no": 4,
            "cooking_procedure_description": "人参は５mmほどの厚みの輪切り、レンコンは大きければ半分に切って、２～３mmの厚さに切り、花型などに飾り切りにする。",
            "holiday_flag": 0,
            "classification_id": 3,
            "classification_name": "野菜カット",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99967,
            "directions_detail_id": 98137,
            "sequence_no": 5,
            "cooking_procedure_description": "Ｂの材料と人参、レンコン（飾り切りの切れ端も一緒に）を鍋に入れ、中火で柔ら煮て冷ます。冷めたら切れ端の部分をみじん切りにする。",
            "holiday_flag": 0,
            "classification_id": 9,
            "classification_name": "煮る",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99968,
            "directions_detail_id": 98138,
            "sequence_no": 6,
            "cooking_procedure_description": "絹さやは筋を取って塩（分量外）茹でにしておく。",
            "holiday_flag": 0,
            "classification_id": 9,
            "classification_name": "煮る",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99969,
            "directions_detail_id": 98139,
            "sequence_no": 7,
            "cooking_procedure_description": "エビは背わたを取り、殻ごと日本酒でサッと洗う。鍋に湯を沸かし、塩（分量外）を少し入れて２～３分ゆで冷ます。冷めたら殻をむく。",
            "holiday_flag": 0,
            "classification_id": 9,
            "classification_name": "煮る",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99970,
            "directions_detail_id": 98140,
            "sequence_no": 8,
            "cooking_procedure_description": "ハマグリは殻を良く洗って日本酒と一緒に鍋に入れ、ふたをして強火にかける。殻が開いたものから取り出す。",
            "holiday_flag": 0,
            "classification_id": 9,
            "classification_name": "煮る",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99971,
            "directions_detail_id": 98141,
            "sequence_no": 9,
            "cooking_procedure_description": "身を殻から取り出し、鍋にしょう油大さじ１とみりん大さじ２を一緒に入れて火にかけ２～３分煮る。",
            "holiday_flag": 0,
            "classification_id": 9,
            "classification_name": "煮る",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99972,
            "directions_detail_id": 98142,
            "sequence_no": 10,
            "cooking_procedure_description": "卵をボールなどに割り入れ、塩を加えて良く溶きほぐし、フライパンにサラダ油を適量入れ、薄焼き卵を焼く。焼き上がったら細く切って錦糸卵を作る。",
            "holiday_flag": 0,
            "classification_id": 2,
            "classification_name": "焼く",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99973,
            "directions_detail_id": 98143,
            "sequence_no": 11,
            "cooking_procedure_description": "米が炊きあがったら昆布を取り出し、上下を返すように混ぜてしばらく蒸らす。蒸らした米を飯台に出し（無ければ大きめのボールなど）寿司酢を回しかけて米になじませるように混ぜる。",
            "holiday_flag": 0,
            "classification_id": 5,
            "classification_name": "その他手作業",
            "cutting_types_id": null
        },
        {
            "directions_detail_pattern_id": 99974,
            "directions_detail_id": 98144,
            "sequence_no": 12,
            "cooking_procedure_description": "みじん切りにしておいたしいたけ、人参、レンコンをすし飯に加えて混ぜ合わせる。器に盛り、レンコン、えび、はまぐり、錦糸卵、絹さや、桜でんぶ（お好みで）でかわいく飾って完成。",
            "holiday_flag": 0,
            "classification_id": 5,
            "classification_name": "その他手作業",
            "cutting_types_id": null
        }
    ],
